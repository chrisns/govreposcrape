{
	"name": "govreposcrape",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"test": "vitest",
		"type-check": "tsc --noEmit",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint --fix src/**/*.ts",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
		"validate:baseline": "./scripts/validate-ai-search-baseline.sh",
		"cost-monitor": "tsx scripts/cost-monitoring.ts",
		"cost-monitor:alert": "tsx scripts/cost-monitoring.ts --alert",
		"cost-monitor:export": "tsx scripts/cost-monitoring.ts --export json",
		"security-audit": "./scripts/security-audit.sh",
		"security-audit:checklist": "./scripts/security-audit.sh --checklist-only",
		"security-audit:dependencies": "./scripts/security-audit.sh --dependencies",
		"security-audit:json": "./scripts/security-audit.sh --format json",
		"metrics-export": "tsx scripts/export-metrics.ts",
		"metrics-export:weekly": "tsx scripts/export-metrics.ts --format json",
		"metrics-export:monthly": "tsx scripts/export-metrics.ts --format json --start-date $(date -v -30d +%Y-%m-%d) --end-date $(date +%Y-%m-%d)",
		"smoke-test": "./scripts/smoke-test.sh",
		"smoke-test:verbose": "./scripts/smoke-test.sh --verbose",
		"smoke-test:test": "./scripts/smoke-test.sh --test",
		"prepare": "husky"
	},
	"devDependencies": {
		"@openapitools/openapi-generator-cli": "^2.25.0",
		"@typescript-eslint/eslint-plugin": "^8.46.4",
		"@typescript-eslint/parser": "^8.46.4",
		"eslint": "^9.39.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"prettier": "^3.6.2",
		"tsx": "^4.20.6",
		"typescript": "^5.5.2",
		"vitest": "~4.0.0"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.25.2"
	}
}
