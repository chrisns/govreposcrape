{
  "name": "govreposcrape-api",
  "version": "1.0.0",
  "description": "Google Cloud Run API for UK Government Repository Search using Vertex AI Search",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "vitest",
    "test:integration": "vitest run test/integration",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "vertex-ai-search",
    "google-cloud-run",
    "uk-government",
    "semantic-search"
  ],
  "author": "CDDO",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@google-cloud/discoveryengine": "^2.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "vitest": "^1.0.4",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
